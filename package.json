    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.0.0",
    "prettier": "3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.6.0"
